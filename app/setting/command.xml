<!-- 串口收发的消息 -->
<command>
    <!-- 到达某个区域 -->
    <arrive>wait</arrive>
    <!-- 微调 -->
    <tweak>kstz</tweak>

    <!-- 准备抓取 -->
    <prepareCatch>run2</prepareCatch>

    <!-- 任务完成 -->
    <complete>rwwc</complete>
    
    <!-- 抓红色 -->
    <catchred>CTHR</catchred>
    <!-- 抓绿色 -->
    <catchgreen>CTHG</catchgreen>
    <!-- 抓蓝色 -->
    <catchblue>CTHB</catchblue>

    <!-- 抓取/放置动作完成 -->
    <mngOK>mnok</mngOK>

    <!-- 到达二维码区附近 -->
    <arriveQR>OK--</arriveQR>
    <!-- 到达原料区附近 -->
    <arriveYL>wait</arriveYL>
    <!-- 二维码识别完成，进行前进 -->
    <qrComplete>QROK</qrComplete>

    <!-- 三个物块都抓取完成 -->
    <task2OK>t2OK</task2OK>
    <!-- 发送微调误差 -->
    
    <!-- 微调动作完成 -->
    <tweakOk>wtok</tweakOk>
    <!-- 校准完成 -->
    <calibrOk>cbok</calibrOk>
    <!-- 向左转弯 -->
    <turnLeft>tl--</turnLeft>
    <!-- 向右转弯 -->
    <turnRight>tr--</turnRight>
    <!-- 进行抓取 -->
    <catch>ctch</catch>
    <!-- 进行放置 -->
    <set>st--</set>

    <!--  -->
</command>